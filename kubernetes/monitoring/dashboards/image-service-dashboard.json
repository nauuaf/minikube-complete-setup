{
  "dashboard": {
    "id": null,
    "title": "Image Service Dashboard",
    "tags": ["image", "microservices"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Image Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(image_requests_total{method=\"POST\"}[5m]))",
            "legendFormat": "Images/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Processing Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(image_processing_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {},
    "templating": {"list": []},
    "annotations": {"list": []},
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 1
  }
}