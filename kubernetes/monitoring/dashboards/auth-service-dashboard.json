{
  "dashboard": {
    "id": null,
    "title": "Auth Service Dashboard",
    "tags": ["auth", "microservices"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Authentication Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(auth_requests_total[5m]))",
            "legendFormat": "Auth Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(auth_success_total) / sum(auth_requests_total) * 100",
            "legendFormat": "Success %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {},
    "templating": {"list": []},
    "annotations": {"list": []},
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 1
  }
}